    <script>
        // Language Toggle with Widget Sync
        const langToggle = document.getElementById('language-toggle');

        // Initialize from localStorage or default to 'en'
        const savedLang = localStorage.getItem('bizdnaii_site_lang') || 'en';
        document.body.setAttribute('data-lang', savedLang);
        langToggle.textContent = savedLang.toUpperCase();

        // Save for widget
        localStorage.setItem('bizdnaii_widget_lang', savedLang);

        langToggle.addEventListener('click', () => {
            const currentLang = document.body.getAttribute('data-lang');
            const newLang = currentLang === 'en' ? 'ru' : 'en';
            document.body.setAttribute('data-lang', newLang);
            langToggle.textContent = newLang.toUpperCase();

            // Save selected language
            localStorage.setItem('bizdnaii_site_lang', newLang);
            localStorage.setItem('bizdnaii_widget_lang', newLang);

            // Notify widget about language change
            window.dispatchEvent(new CustomEvent('bizdnaii-language-change', {
                detail: { language: newLang }
            }));
        });

        // Hamburger Menu Toggle
        document.querySelector('.hamburger').addEventListener('click', () => {
            document.querySelector('.nav-links').classList.toggle('active');
        });

        // Smooth Scroll for Anchor Links
        document.querySelectorAll('a[href^="#"]').forEach(anchor => {
            anchor.addEventListener('click', function (e) {
                e.preventDefault();
                document.querySelector(this.getAttribute('href')).scrollIntoView({ behavior: 'smooth' });
            });
        });

        // Canvas Neural Network Animation
        const c = document.getElementById('neural-bg');
        const ctx = c.getContext('2d');
        let w, h, lines = [];
        function resize() {
            w = c.width = window.innerWidth;
            h = c.height = window.innerHeight;
        }
        window.addEventListener('resize', resize);
        resize();
        function initLines() {
            lines = [];
            for (let i = 0; i < 55; i++) {
                lines.push({
                    x: Math.random() * w,
                    y: Math.random() * h,
                    dx: (Math.random() - 0.5) * 0.6,
                    dy: (Math.random() - 0.5) * 0.6
                });
            }
        }
        initLines();
        function draw() {
            ctx.clearRect(0, 0, w, h);
            for (let i = 0; i < lines.length; i++) {
                let a = lines[i];
                a.x += a.dx;
                a.y += a.dy;
                if (a.x < 0 || a.x > w) a.dx *= -1;
                if (a.y < 0 || a.y > h) a.dy *= -1;
                ctx.beginPath();
                ctx.arc(a.x, a.y, 1.2, 0, Math.PI * 2);
                ctx.fillStyle = "#00D4FF";
                ctx.fill();
                for (let j = i + 1; j < lines.length; j++) {
                    let b = lines[j], dx = a.x - b.x, dy = a.y - b.y, dist = Math.sqrt(dx * dx + dy * dy);
                    if (dist < 120) {
                        ctx.beginPath();
                        ctx.moveTo(a.x, a.y);
                        ctx.lineTo(b.x, b.y);
                        ctx.strokeStyle = `rgba(0,255,255,${(1 - dist / 120) * 0.3})`;
                        ctx.lineWidth = 1;
                        ctx.stroke();
                    }
                }
            }
            requestAnimationFrame(draw);
        }
        draw();
    </script>
